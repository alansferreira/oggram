{
  "name": "oggram-api",
  "version": "1.2.2",
  "description": "",
  "main": "./dist/index.js",
  "author": "Alan S. Ferreira",
  "license": "MIT License",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alansferreira/oggram.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "ttsc --build",
    "start:dev": "nodemon",
    "start": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env JEST_PROFILE=e2e jest",
    "test:unit": "cross-env JEST_PROFILE=unit jest",
    "post:release": "git push --follow-tags origin && git push heroku",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "colors": "^1.4.0",
    "dotenv": "16.0.1",
    "dotenv-expand": "^8.0.3",
    "express": "^4.17.2",
    "mongodb": "^4.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/conventional-changelog-config-spec": "^2.1.1",
    "@types/jest": "27.0.3",
    "@types/node": "^16.0.0",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^7.0.4",
    "jest": "27.3.1",
    "jest-html-reporter": "3.4.2",
    "jest-junit": "13.0.0",
    "nodemon": "^2.0.19",
    "prettier": "2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "ts-patch": "^2.0.1",
    "tsconfig-paths": "^3.11.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": "^16.0.0"
  }
}
